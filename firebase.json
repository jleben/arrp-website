{
  "hosting": {
    "public": "public",
    "cleanUrls": true,

    "rewrites": [{
      "source": "/compiler",
      "run": {
        "serviceId": "arrp-compiler-v1",
        "region": "us-central1"
      }
    },{
      "source": "**",
      "destination": "/index.html"
    }],

    "headers": [{
        "source": "**",
        "headers": [{
          "key": "Cache-Control",
          "value": "no-cache"
        }]
    }],

    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
